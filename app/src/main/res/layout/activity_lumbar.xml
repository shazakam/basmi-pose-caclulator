<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LumbarActivity">

    <ImageView
        android:id="@+id/lumbarNeutralExample"
        android:layout_width="160dp"
        android:layout_height="160dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.041"
        app:srcCompat="@drawable/lumbar_neutral_1" />

    <ImageView
        android:id="@+id/lumbarRightExample"
        android:layout_width="160dp"
        android:layout_height="160dp"
        app:layout_constraintBottom_toBottomOf="@+id/lumbarNeutralExample"
        app:layout_constraintStart_toEndOf="@+id/lumbarNeutralExample"
        app:layout_constraintTop_toTopOf="@+id/lumbarNeutralExample"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/lumbar_right_1" />

    <ImageView
        android:id="@+id/lumbarLeftExample"
        android:layout_width="180dp"
        android:layout_height="160dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@+id/lumbarRightExample"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="@+id/lumbarNeutralExample"
        app:layout_constraintTop_toBottomOf="@+id/lumbarNeutralExample"
        app:srcCompat="@drawable/lumbar_left_1" />

    <Button
        android:id="@+id/btnLumbarNext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:onClick="onLumbarNextClick"
        android:text="Next"
        app:backgroundTint="#121111"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.987"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnNeutralLumbarUpload"
        android:layout_width="115dp"
        android:layout_height="80dp"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="52dp"
        android:onClick="onClickLumbarUpload"
        android:text="Upload Neutral Position"
        app:backgroundTint="#100F0F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnRightLumbarUpload"
        android:layout_width="115dp"
        android:layout_height="80dp"
        android:layout_marginStart="12dp"
        android:layout_marginEnd="12dp"
        android:onClick="onClickLumbarUpload"
        android:text="Upload Right Position"
        app:backgroundTint="#131212"
        app:layout_constraintBottom_toBottomOf="@+id/btnNeutralLumbarUpload"
        app:layout_constraintEnd_toStartOf="@+id/btnLeftLumbarUpload"
        app:layout_constraintStart_toEndOf="@+id/btnNeutralLumbarUpload"
        app:layout_constraintTop_toTopOf="@+id/btnNeutralLumbarUpload"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnLeftLumbarUpload"
        android:layout_width="115dp"
        android:layout_height="80dp"
        android:layout_marginEnd="4dp"
        android:onClick="onClickLumbarUpload"
        android:text="Upload Left Position"
        app:backgroundTint="#121111"
        app:layout_constraintBottom_toBottomOf="@+id/btnRightLumbarUpload"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/btnRightLumbarUpload"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/lumbarNeutralExampleText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Neutral Position"
        app:layout_constraintEnd_toEndOf="@+id/lumbarNeutralExample"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toStartOf="@+id/lumbarNeutralExample"
        app:layout_constraintTop_toBottomOf="@+id/lumbarNeutralExample" />

    <TextView
        android:id="@+id/lumbarRightExampleText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Right Position"
        app:layout_constraintEnd_toEndOf="@+id/lumbarRightExample"
        app:layout_constraintHorizontal_bias="0.568"
        app:layout_constraintStart_toEndOf="@+id/lumbarNeutralExample"
        app:layout_constraintTop_toBottomOf="@+id/lumbarRightExample" />

    <TextView
        android:id="@+id/lumbarLeftExampleText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Left Position"
        app:layout_constraintEnd_toEndOf="@+id/lumbarLeftExample"
        app:layout_constraintStart_toStartOf="@+id/lumbarLeftExample"
        app:layout_constraintTop_toBottomOf="@+id/lumbarLeftExample" />

    <TextView
        android:id="@+id/rightLumbarExtensionPhys"
        android:layout_width="93dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="188dp"
        android:text="Right Lumbar Extension"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/leftLumbarExtensionPhysInput"
        android:layout_width="69dp"
        android:layout_height="49dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toBottomOf="@+id/leftLumbarExtensionPhys"
        app:layout_constraintStart_toEndOf="@+id/leftLumbarExtensionPhys"
        app:layout_constraintTop_toTopOf="@+id/leftLumbarExtensionPhys"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/leftLumbarExtensionPhys"
        android:layout_width="93dp"
        android:layout_height="38dp"
        android:layout_marginStart="4dp"
        android:text="Left Lumbar Extension"
        app:layout_constraintBottom_toBottomOf="@+id/rightLumbarExtensionPhysInput"
        app:layout_constraintStart_toEndOf="@+id/rightLumbarExtensionPhysInput"
        app:layout_constraintTop_toTopOf="@+id/rightLumbarExtensionPhysInput"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/rightLumbarExtensionPhysInput"
        android:layout_width="69dp"
        android:layout_height="48dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toBottomOf="@+id/rightLumbarExtensionPhys"
        app:layout_constraintStart_toEndOf="@+id/rightLumbarExtensionPhys"
        app:layout_constraintTop_toTopOf="@+id/rightLumbarExtensionPhys"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/rightLumbarNeutralPhys"
        android:layout_width="93dp"
        android:layout_height="38dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="252dp"
        android:text="Right Lumbar Neutral"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/rightLumbarNeutralPhysInput"
        android:layout_width="69dp"
        android:layout_height="49dp"
        android:layout_marginStart="8dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toBottomOf="@+id/rightLumbarNeutralPhys"
        app:layout_constraintStart_toEndOf="@+id/rightLumbarNeutralPhys"
        app:layout_constraintTop_toTopOf="@+id/rightLumbarNeutralPhys"
        app:layout_constraintVertical_bias="0.636" />

    <EditText
        android:id="@+id/leftLumbarNeutralPhysInput"
        android:layout_width="69dp"
        android:layout_height="49dp"
        android:ems="10"
        android:inputType="numberDecimal"
        app:layout_constraintBottom_toBottomOf="@+id/leftLumbarNeutralPhys"
        app:layout_constraintStart_toEndOf="@+id/leftLumbarNeutralPhys"
        app:layout_constraintTop_toTopOf="@+id/leftLumbarNeutralPhys"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/leftLumbarNeutralPhys"
        android:layout_width="93dp"
        android:layout_height="38dp"
        android:layout_marginStart="4dp"
        android:text="Left Lumbar Neutral"
        app:layout_constraintBottom_toBottomOf="@+id/rightLumbarNeutralPhysInput"
        app:layout_constraintStart_toEndOf="@+id/rightLumbarNeutralPhysInput"
        app:layout_constraintTop_toTopOf="@+id/rightLumbarNeutralPhysInput"
        app:layout_constraintVertical_bias="0.636" />

    <Button
        android:id="@+id/submitLumbar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="132dp"
        android:onClick="onLumbarMeasurmentsClick"
        android:text="submit Lumbar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>